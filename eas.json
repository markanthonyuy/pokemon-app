{
  "cli": {
    "version": ">= 7.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
        }
      },
      "ios": {
        "env": {
          "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
        }
      }
    },
    "preview-playstore": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
        }
      },
      "ios": {
        "env": {
          "EXPO_PUBLIC_GQL_ENDPOINT": "https://beta.pokeapi.co/graphql/v1beta"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
